{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "Robot": true,
    "Line": true,
    "Text": true,
    "getConfigs": true,
    "World": true,
    "GuessInput": true,
    "ScoreInput": true,
    "Game": true,
    "COLORS": true,
    "KEYS": true,
    "InputItem": true,
    "console": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "no-debugger": 0,
    "quotes": [
      1,
      "single"
    ],
    "semi": 1
  }
}
